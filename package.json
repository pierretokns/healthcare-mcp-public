    "mcp",
    "fda",
    "pubmed",
    "clinical-trials",
    "icd-10",
    "mcp-server"
  ],
  "author": "Cicatriiz",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "node-fetch": "^3.3.2",
    "node-cache": "^5.1.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "wrangler": "^3.0.0",
    "@cloudflare/workers-types": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
